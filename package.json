{
  "name": "ProtractorFramework",
  
  "dependencies":{
    "protractor": "^7.0.0",
    "jasmine-data-provider": "^2.2.0",
    "protractor-jasmine2-html-reporter":"^0.0.7"
  },
  
  "scripts": {
    "webdriver-update": "./node_modules/.bin/webdriver-manager update",
    "webdriver-start": "./node_modules/.bin/webdriver-manager start",
    "protractor": "./node_modules/.bin/protractor Configuration.js",
    "start": "npm run webdriver-update &&npm run webdriver-start &&npm run protractor",
    "run-protractor": "npm run protractor",
    "smokeTestSuite": "./node_modules/.bin/protractor Configuration.js --suite SmokeTest",
    "regressionTestSuite": "./node_modules/.bin/protractor Configuration.js --suite RegressionTest"
  }
  
}
